{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:modern": "nuxt build --modern=server",
    "start:modern": "nuxt start --modern=server",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore src/",
    "lint:prettier": "prettier --check src/",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different src/ && npm run lint:js --fix"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.1.72",
    "@nuxt/devtools": "latest",
    "@vee-validate/yup": "^4.12.5",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nuxt": "^3.10.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "yup": "^1.3.3"
  },
  "dependencies": {
    "@nuxt/image": "^1.3.0",
    "@nuxt/ui": "^2.13.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@vee-validate/nuxt": "^4.12.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "nuxt-icon": "^0.6.8",
    "sass": "^1.70.0",
    "vue3-carousel-nuxt": "^1.1.0",
    "vue3-google-map": "^0.18.0"
  }
